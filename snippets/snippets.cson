".html.text.vue":
  "za-loading":
    "prefix":"zaLoading",
    "body":"""
      <za-loading :visible.sync=''>${1:content}</za-loading>
    """,
    "description":""

  "za-toast":
    "prefix":"zaToast",
    "body":"""
      <za-toast
      :visible.sync=''
      @close=''
      :duration=''>
      </za-toast>
    """,
    "description":""

  "za-swipe-action":
    "prefix":"zaSwipeAction",
    "body":"""
      <za-swipe-action :right='' :left=''>
        <za-cell>${1:content}</za-cell>
      </za-swipe-action>
    """,
    "description":""

  "za-pull":
    "prefix":"zaPull",
    "body":"""
     <za-pull :on-refresh='' :refreshing=''>
      <za-cell v-for='i in myData' :key='i'>${1:content}</za-cell>
    </za-pull>
    """,
    "description":""

  "za-popup":
    "prefix":"zaPopup":
    "body":"""
        <za-popup
          :visible.sync=''
          direction=''
          :duration=''
          maskType=''
          @close=''>
          <div>${1:content}</div>
        </za-popup>
    """,
    "description":""

  "za-modal":
    "prefix":"zaModal":
    "body":"""
        <za-modal :visible.sync='' @close='' title='' :showClose=''>
          ${1:content}
        </za-modal>
    """,
    "description":""

  "za-button":
    "prefix":"zaButton",
    "body":"""
      <za-button theme='' size='' shape=''>${1:conent}</za-button>
    """,
    "description":""

  "za-actionsheet":
    "prefix":"zaActionSheet",
    "body":"""
      <za-actionsheet
        :visible.sync=''
        :actions=''
        :showCancel=''
        @cancel=''>
      </za-actionsheet>
    """,
    "description":""

".meta.tag":
  "v-za-loading":
    "prefix":"v-za-loading",
    'body': 'v-za-loading=\"${1:condition}\"'

".source.js":
    "zaToast":
      "prefix": "zaToast",
      "body": """
      this.$zaToast('${1:text}',{
        duration: '${2:duration}'
      });
      """
